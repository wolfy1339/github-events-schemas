{
  "name": "github-events-schemas",
  "version": "0.0.0-development",
  "description": "machine-readable, always up-to-date GitHub Events specifications",
  "main": "payload-examples/api.github.com/index.json",
  "files": [
    "payload-examples/api.github.com/index.json"
  ],
  "scripts": {
    "build": "ts-node -T bin/octokit-gh-events.ts check --cached",
    "build:webhooks": "ts-node -T bin/octokit-gh-events.ts update",
    "lint": "prettier --check '{bin,lib}/**/*.ts' '*.{md,json}' 'bin/docs/*.md' '{payload-examples,payload-schemas,payload-types}/**/*.{ts,md,json,d.ts}'",
    "lint:fix": "prettier --write '{bin,lib}/**/*.ts' '*.{md,json}' 'bin/docs/*.md' '{payload-examples,payload-schemas,payload-types}/**/*.{ts,md,json,d.ts}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolfy1339/github-events-schemas.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolfy1339/github-events-schemas/issues"
  },
  "homepage": "https://github.com/wolfy1339/github-events-schemas#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/json-diff": "^0.7.0",
    "@types/node": "^17.0.8",
    "@types/prettier": "^2.4.2",
    "@types/turndown": "^5.0.1",
    "@types/yargs": "^17.0.8",
    "cheerio": "^1.0.0-rc.10",
    "got": "^11.8.3",
    "json-diff": "^0.7.1",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.1",
    "semantic-release-plugin-update-version-in-files": "^1.1.0",
    "ts-node": "^10.4.0",
    "turndown": "^7.1.1",
    "typescript": "^4.5.4",
    "yargs": "^17.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "package.json"
          ]
        }
      ],
      "@semantic-release/npm"
    ]
  },
  "renovate": {
    "extends": [
      "github>octokit/.github"
    ]
  }
}
